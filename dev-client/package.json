{
  "name": "Terraso LandPKS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint ./ --ext .js --ext .ts --ext .tsx --max-warnings 0 --ignore-path ../.gitignore",
    "lint-js": "npm run lint",
    "format": "npm run lint -- --fix",
    "format-js": "npm run format",
    "start": "expo start",
    "test": "jest",
    "check-ts": "tsc --noEmit",
    "check-modules": "depcheck",
    "prebuild": "expo prebuild"
  },
  "dependencies": {
    "@craftzdog/react-native-buffer": "^6.0.5",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^4.6.1",
    "@react-native-community/checkbox": "^0.5.17",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/material-top-tabs": "^6.6.13",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@rnmapbox/maps": "^10.1.24",
    "@sentry/react-native": "^5.22.2",
    "base-64": "^1.0.0",
    "delta-e": "^0.0.8",
    "expo": "~51.0.7",
    "expo-auth-session": "~5.5.2",
    "expo-av": "~14.0.5",
    "expo-camera": "~15.0.8",
    "expo-constants": "~16.0.1",
    "expo-crypto": "~13.0.2",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.0.5",
    "expo-media-library": "~16.0.3",
    "expo-screen-orientation": "~7.0.5",
    "expo-sensors": "~13.0.7",
    "formik": "^2.4.6",
    "haversine": "^1.1.1",
    "i18next": "^23.11.4",
    "jpeg-js": "^0.4.4",
    "munsell": "^1.1.5",
    "native-base": "^3.4.28",
    "quantize": "^1.0.2",
    "react": "18.2.0",
    "react-i18next": "^14.1.1",
    "react-native": "^0.74.1",
    "react-native-autocomplete-input": "^5.4.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-gradle-plugin": "^0.71.19",
    "react-native-mmkv-storage": "^0.9.1",
    "react-native-pager-view": "^6.3.1",
    "react-native-paper": "^5.12.3",
